# API Configuration
VITE_API_BASE_URL=http://localhost:3000/api

# ArcGIS Configuration
# Get your API key from: https://developers.arcgis.com/api-keys/
VITE_ARCGIS_API_KEY=your_arcgis_api_key_here

# Map Configuration (Ho Chi Minh City, Vietnam)
VITE_DEFAULT_CENTER_LON=106.6297
VITE_DEFAULT_CENTER_LAT=10.8231
VITE_DEFAULT_ZOOM=15

# Optional: WebScene Configuration
# VITE_WEBSCENE_ID=your_webscene_id_here

# Feature Flags
VITE_ENABLE_MESH_RENDERING=true
VITE_ENABLE_3D_MODELS=true
VITE_ENABLE_FILE_UPLOAD=true

# Development
VITE_DEV_MODE=true
VITE_ENABLE_MOCK_DATA=false
